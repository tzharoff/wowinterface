["customText"] = "function(_,_,prog,_,name)\n    if aura_env.state.type == \"alert\" then\n        prog = aura_env.config.showProg and prog or \"\"\n        return format(\"%s %s\", name, prog)\n    elseif aura_env.state.type == \"main\" and aura_env.start then \n        local combatTime = GetTime() - aura_env.start\n        return WeakAuras.IsClassic() and GetTimeStringFromSeconds(combatTime, false, true) \n        or SecondsToClock(combatTime, false) \n    end\nend\n\n\n\n\n\n\n\n\n\n\n\n\n",
