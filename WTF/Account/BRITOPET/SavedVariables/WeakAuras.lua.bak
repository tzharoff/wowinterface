["events"] = "ALERT_START START",
["custom"] = "function(allstates, event, index, settings)\n    if event == \"START\" then\n        if aura_env.timers then\n            for _, timer in pairs(aura_env.timers) do\n                timer:Cancel()       \n            end\n        end\n        aura_env.start = GetTime()\n        aura_env.timers = {}\n        local aura_env = aura_env\n        for i, settings in ipairs(aura_env.config.alerts) do\n            if settings.time > 0 then\n                local timer = C_Timer.NewTimer(settings.time, function()\n                        WeakAuras.ScanEvents(\"ALERT_START\", i, settings)\n                    end\n                )\n                table.insert(aura_env.timers, timer)\n            end\n        end \n        if aura_env.start and aura_env.config.showMain then\n            local combatTime = GetTime() - aura_env.start\n            allstates[\"maintimer\"] = {\n                show = true,\n                changed = true, \n                type = \"main\",\n                index = 0,\n            }\n        end\n        return true\n    elseif event == \"ALERT_START\" and index then\n        local col = CreateColor(unpack(settings.colour))\n        local icon = settings.icon ~= \"\" and \"|T\"..settings.icon..\":0|t\" or \"\"\n        local dur = settings.duration\n        local exp = GetTime() + dur\n        allstates[index] = {\n            show = true,\n            changed = true,\n            name = icon .. col:WrapTextInColorCode(settings.text),\n            type = \"alert\",\n            progressType = \"timed\",\n            duration = dur,\n            expirationTime = exp,\n            autoHide = true,\n            index = exp,\n            alert = index,\n            text = settings.text,\n        }\n        return true\n    end\n    \nend",
